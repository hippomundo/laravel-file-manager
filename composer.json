{
  "name": "hippomundo/laravel-file-manager",
  "description": "The best way to upload, store and manage your files.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "files",
    "file",
    "video",
    "videos",
    "images",
    "image",
    "media",
    "videos",
    "video",
    "photos",
    "photo",
    "manager",
    "uploader",
    "resize",
    "rotate"
  ],
  "authors": [
    {
      "name": "Roman Gilyov",
      "email": "rgilyov@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "^5.1|^6.0|^7.0|^8.0|^9.0",
    "intervention/image": "^2.3",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^3.2",
    "orchestra/database": "^3.2",
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Hippomundo\\FileManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hippomundo\\FileManager\\Test\\": "tests/"
    }
  }
}
